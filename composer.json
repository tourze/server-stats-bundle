{
  "name": "tourze/server-stats-bundle",
  "description": "功能强大的 Symfony Bundle，用于收集、存储和可视化全面的服务器统计信息和监控数据，包括 CPU、内存、网络流量、磁盘 I/O 等系统指标",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/collections": "^2.3",
    "doctrine/data-fixtures": "^2.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "monolog/monolog": "^3.1",
    "nesbot/carbon": "^2.72 || ^3",
    "psr/log": "^3|^2|^1",
    "symfony/cache": "^7.3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/arrayable": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/doctrine-timestamp-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/gb-t-2659": "1.0.*",
    "tourze/server-node-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*",
    "tourze/symfony-schedule-entity-clean-bundle": "1.0.*",
    "yiisoft/arrays": "^3",
    "yiisoft/json": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/security-core": "^7.3",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "ServerStatsBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ServerStatsBundle\\Tests\\": "tests"
    }
  }
}